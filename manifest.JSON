{
  "manifest_version": 3,
  "name": "Digital Diary",
  "description": "Mood Tracker, Positive Affirmations, Manifestation ",
  "version": "1.0",
  "action": {
    "default_popup": "DigitalDiary.html",
    "default_icon": "assets/icon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ],
  "name": "Permissions Extension",
  "permissions": [
    "storage", 
    "alarms", 
    "notifications" 
  ],
  "host_permissions": [
    "https://www.affirmations.dev/"
  ],
  "background": {
    "service_worker" : "background.js"
  },
"action": {
  "default_popup": "content.js"
}
}

